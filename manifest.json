{
  "manifest_version": 3,
  "name": "Epoch Goals",
  "version": "1.0.0",
  "description": "Beautiful goals and tasks on every new tab",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
